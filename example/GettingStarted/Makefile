all: gen_config

###===================================================================
### pre
###===================================================================

tdata: ../../tdata
	cp ../../tdata .

###===================================================================
### build
###===================================================================
.PHONY: co compile

ebin:
	mkdir ebin

co:compile
compile: ebin
	erlc -W0 -o ebin src/*.erl

gen_config: compile tdata
	./tdata -f
